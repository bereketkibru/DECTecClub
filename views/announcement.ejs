<!-- <%- include("partials/header"); -%> -->
<%var j=0%>
<%var ann=[]%>
<%var pDate=[]%>
<%var antitle=[]%>

<%  announcements.forEach(function(announcement){ %>
<%antitle.push(announcement.atitle)%>
        <%ann.push(announcement.acontent) %>
        <%pDate.push(announcement.duDate) %>
        <% i++ %>
        <% }) %>
        <%var x=antitle.length%>

        <div class="container">
          <div class="row">
            <div class="col-md-8">
<% for(var i=x-1;i>x-10;i--){%>
  <div class="card border-info">
    <div class="card-header">
      <h4 class="card-title float-left text-info"><%= antitle[i]%></h4>
      
      <div class="spinner-grow text-info float-right" role="status"></div>
    </div>

    <div class="card-body">
    <p class="card-text"><%= ann[i]%></p>
  </div>
  <div class="card-footer text-muted">

    
    <div class="float-right">
      <%= pDate[i]%>
    </div>
  </div>
</div>
<%}%>
</div>
</div>
</div>


<script src="./js/jquery-3.2.1.min.js"></script>
