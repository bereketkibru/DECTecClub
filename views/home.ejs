    <%var pos=[]%>
    <%var pimag=[]%>
    <%var ptitle=[]%>
    <%var pDate=[]%>
    <%var pByh=[]%>

    
  <%  posts.forEach(function(post){ %>
    <%ptitle.push(post.title)%>
    <%pos.push(post.content) %>
    <%pimag.push(post.imag) %>
    <%pDate.push(post.date) %>
    <%pByh.push(post.pBy) %>
    <% i++ %>

    <% }) %>
    <%var y=ptitle.length%>
    <%var x=y-10%>
    <div class="container">
      <div class="row">
        <div class="col-md-8">
    <% for(var i=y-1;i>x+1;i--){%>
      <div class="card">
        <div class="card-header">
          <h4 class="card-title float-left"><%=ptitle[i]%></h4>
        </div>
          <img src="./Data/Post/<%=pimag[i]%>" " alt="...">
          
        <div class="card-body">
          <p id="contact-intro" class="card-text"><%=pos[i]%> </p>
          
        </div>
        <div class="card-footer text-muted">
          <% q=pDate[i].toString()%>
          <%var w=q.substring(3,16)%>
          
          <div class="float-left">
              <cite class="text-muted">@<a href="/uProfile/<%=pByh[i]%>" class="text-muted"><%=pByh[i]%></a></cite>
            
          </div>
          <div class="float-right">
            <%=w %>
          </div>
        </div>
    <%}%>
  </div>
</div>
</div>
 



    <script src="./js/jquery-3.2.1.min.js"></script>
    
        <script src="./js/umd/popper.min.js"></script>
        <script src="./b/js/bootstrap.min.js"></script>
